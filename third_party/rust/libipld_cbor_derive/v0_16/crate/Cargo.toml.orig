[package]
name = "libipld-cbor-derive"
version = "0.16.0"
authors = ["David Craven <david@craven.ch>"]
edition = "2021"
license = "MIT OR Apache-2.0"
description = "ipld cbor codec proc macro"
repository = "https://github.com/ipfs-rust/rust-ipld"

[lib]
proc-macro = true

[dependencies]
proc-macro-crate = "1.1.0"
